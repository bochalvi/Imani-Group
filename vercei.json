{
    "version": 2,
    "builds": [
    {
      "src": "imani_group/wsgi.py",
      "use": "@vercel/python",
      "config": {
                "maxLambdaSize": "15mb", "route": "python3.9"
      } 
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "staticfiles_build"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "imani_group/wsgi.py"
    }
    ]
}